# Supabase
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# AI Provider (openai or anthropic)
AI_PROVIDER=openai
# Default model used by AI_PROVIDER (optional)
AI_MODEL_DEFAULT=gpt-5-mini
# Override models by content type (optional)
AI_MODEL_URL=gpt-5-mini
AI_MODEL_DOCUMENT=gpt-5-mini
AI_MODEL_IMAGE=gpt-5-mini

# OpenAI (required if AI_PROVIDER=openai, or for embeddings with anthropic)
OPENAI_API_KEY=sk-...

# Anthropic (required if AI_PROVIDER=anthropic)
ANTHROPIC_API_KEY=sk-ant-...

# Firecrawl
FIRECRAWL_API_KEY=fc-...

# Apify
APIFY_API_TOKEN=apify_api_...

# yt-dlp (local path, required if using yt-dlp transcript fallback)
YT_DLP_PATH=/opt/homebrew/bin/yt-dlp

# FAL (Whisper transcription fallback)
FAL_KEY=fal_...

# summarize-core model override (optional)
SUMMARIZE_MODEL=gpt-4o-mini

# Sweetistics
SWEETISTICS_API_KEY=sweet-...

# Server
PORT=3000
